<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.admin.ystablayout.MainActivity">

    <com.example.admin.ystablayout.view.YSTabLayout
        android:id="@+id/tablayout"
        android:layout_width="match_parent"
        android:layout_height="47dp"
        app:ys_defaultoption="0"
        app:ys_lineColor="#FFC125"
        app:ys_lineHeight="3dp"
        app:ys_margin="5dp"
        app:ys_noSelectColor="#F8F8FF"
        app:ys_selectColor="#FFC125"
        app:ys_selectTextSize="16dp"
        app:ys_textSize="16dp"
        app:ys_backgroundColor="#4F94CD"
        app:ys_overstriking="true">

    </com.example.admin.ystablayout.view.YSTabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/viewpage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/tablayout">

    </android.support.v4.view.ViewPager>
</RelativeLayout>
